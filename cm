rm -rf build
mkdir build
cd build
export CMAKE_PREFIX_PATH=$NETP/fms.2023.04:$NETP/MAPL-2.53.0/:$NETP:$PREFIX
export CMAKE_MODULE_PATH=$PREFIX/ESMF_8_8_10
export esmf_ROOT=$PREFIX/ESMF_8_8_10 
ls -l $CMAKE_MODULE_PATH
read a
cmake ..  -DAPP=S2SWA -D32BIT=ON -DCCPP_SUITES=FV3_GFS_v17_p8,FV3_GFS_v17_coupled_p8 -DMPI=ON -DCMAKE_BUILD_TYPE=Release -DMOM6SOLO=ON
make -j 8 
sh -x ../bu
make -j 8 VERBOSE=1
cp ufs_model ../tests/ufs_model.x
