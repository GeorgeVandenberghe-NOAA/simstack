rm -rf build
mkdir build
cd build
export CMAKE_PREFIX_PATH=$NETP/$NETP/MAPL-2.53.4/:$NETP:$PREFIX
export CMAKE_MODULE_PATH=$PREFIX/ESMF_8_8_10
export esmf_ROOT=$PREFIX/ESMF_8_8_10 
ls -l $CMAKE_MODULE_PATH
export NETPR=`dirname $NETP`
cmake ..  -DAPP=S2SW -D32BIT=ON -DCCPP_SUITES=FV3_GFS_v17_p8,FV3_GFS_v17_coupled_p8 -DMPI=ON -DCMAKE_BUILD_TYPE=Release -DMOM6SOLO=ON
make -j 8 
# COMPILE MOM SOURCE THAT WON't COMPILE WITH ifx then rebuild every thing else
sh -x ../bu
make -j 8 VERBOSE=1
cp ufs_model ../tests/ufs_model.x
